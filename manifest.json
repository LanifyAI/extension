{
  "manifest_version": 3,
  "name": "Lanify",
  "description": "Leverage your unused internet bandwidth!",
  "homepage_url": "https://lanify.ai/",
  "key": "",
  "action": {
    "default_popup": "src/pages/popup/index.html",
    "default_icon": {
      "32": "icon-32.png"
    }
  },
  "icons": {
    "128": "icon-128.png"
  },
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  },
  "permissions": [
    "activeTab",
    "storage",
    "background"
  ],
  "web_accessible_resources": [
    {
      "matches": [],
      "resources": [
        "contentStyle.css",
        "icon-128.png",
        "icon-32.png"
      ],
      "use_dynamic_url": false
    }
  ],
  "externally_connectable": {
    "matches": [
      "http://127.0.0.1/*",
      "http://localhost/*",
      "*://*.lanify.ai/*"
    ]
  },
  "host_permissions": [
    "<all_urls>"
  ],
  "version": "0.1.0"
}